<!DOCTYPE html>
<html>
  <head>
    <title>AuthAuth</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all' %>
    <%= javascript_include_tag 'application' %>
  </head>

  <body>
    <% flash.each do |type, message| %>
      <%= sanitize(message) %>
    <% end %>
    <%= yield %>
  </body>
</html>
