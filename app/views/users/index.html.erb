<h1>Users Index Page</h1>

<div class="links">
  <%= link_to "Register as User", new_user_path %>
  <% if session[:user_id] == nil %>
  <%= link_to "Log In", login_path %>
  <% end %>
  <% if session[:user_id] != nil %>
  <%= link_to "Log Out", logout_path, method: :delete, session: {user_id: session[:user_id]} %>
  <% end %>
</div>

<div class="user-info">
  <% @users.each do |user| %>
  <p><%= user.full_name %></p>
  <p><%= user.email_address %></p>
  <% end %>
</div>
